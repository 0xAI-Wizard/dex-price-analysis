    const inkeepWidget = Inkeep().embed({
      componentType: "ChatButton",
      colorModeSync: {
        observedElement: document.documentElement,
        isDarkModeCallback: (el) => {
          const currentTheme = el.getAttribute("data-color-mode");
          return currentTheme === "dark";
        },
        colorModeAttribute: "data-color-mode",
      },
      properties: {
        chatButtonType: "PILL",
        baseSettings: {
          apiKey: "74c4e0882793086599a766e03758885768207f811a5868ce", // required
          integrationId: "cm2l6j9vy00dcrt8ydgzpjpfj", // required
          organizationId: "cm1pvmpof05ivt1td6k4bqk2j", // required
          primaryBrandColor: "#35af00", // your brand color, widget color scheme is derived from this
          organizationDisplayName: "CoinGecko",
          // ...optional settings
        },
        modalSettings: {
          // optional settings
        },
        searchSettings: {
          // optional settings
        },
        aiChatSettings: {
          // optional settings
          botAvatarSrcUrl: "https://static.coingecko.com/s/thumbnail-d5a7c1de76b4bc1332e48227dc1d1582c2c92721b5552aae76664eecb68345c9.png", // use your own bot avatar
          quickQuestions: [
            "Using API, how do i get the latest price of a coin?",
            "I have passed in the coin name into the API, but i am getting the wrong price, why is that?",
            "Can i get the historical price chart of a coin?",
          ],
        },
      },
    });
  };

  inkeepScript.addEventListener("load", () => {
     addInkeepWidget(); // initialize the widget    
  });
